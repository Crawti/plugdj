color: #1C1F25;
width: 0px;
height: 0px;
border-radius: 0px;
}
#chat .delete-button {
  font-size: 8px !important;
  padding: 4px;
  top: -12px;
  text-indent: 9999px;
  white-space: nowrap;
  width: 10px;
  top: auto;
}
#volume .slider {
 display: block !important;
}
#chat .delete-button:before {
  position: absolute;
  content: "x";
  display: inline-block;
  text-indent: 0px;
  font-size: 12px;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  text-align: center;
}
#chat-messages .kouhai .text, #chat-messages .senpai .text {
 padding-right:0;
}
#chat .rsshit.context .timestamp, #chat .moderation .timestamp {
 display:none !important;
 }
